#!/usr/bin/node

class Rectangle {
  constructor(w, h) {
      if (w <= 0 || h <= 0 || typeof w !== 'number' || typeof h !== 'number'){
	  // Creating an empty instance with undefined width and height
	  const emptyRectangle = new Rectangle();
	  emptyRectangle.width = undefined;
	  emptyRectangle.height = undefined;
	  return emptyRectangle;
      } else {
	this.width = w;
	this.height = h;
    }
  }
}

module.exports = Rectangle;
