#!/usr/bin/python3
def uppercase(str):
    upper_str = ""
    for ch in str:
        ascii_val = ord(ch)
        if ascii_val >= 97 and ascii_val <= 122:
            upper_ch = chr(ascii_val - 32)
            upper_str += upper_ch
            #print("{:s}".format(upper_ch), end="")
        else:
            upper_str += ch
            #print("{:s}".format(ch), end="")
    print(upper_str)
